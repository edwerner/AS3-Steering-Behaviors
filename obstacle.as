package{	import flash.display.MovieClip;		public class obstacle extends MovieClip	{				public var rad:Number;		private var col:uint;		public function obstacle(radius:Number, color:uint = 0x0000FF)				{			rad = radius;			col = color;			graphics.lineStyle(0, color);			graphics.drawCircle(0, 0, rad);   		}		public function getRadius():Number		{			return rad;		}		public function get position():Vector2		{			return new Vector2(x,y);		}	}}